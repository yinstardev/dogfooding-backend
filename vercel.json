{
    "version": 2,
    "builds": [
      {
        "src": "index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": "source/**/*.ts"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.ts"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors 'self' https://identity.thoughtspotstaginglogin.cloud"
          }
        ]
      }
    ]
  }
  